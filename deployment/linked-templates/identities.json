{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "variables": {
        "location": "[resourceGroup().location]",
        "identityAppGwName": "appGwIdentity",
        "identityClusterName": "clusterIdentity"
    },
    "resources": [
        {
            "type": "Microsoft.ManagedIdentity/userAssignedIdentities",
            "apiVersion": "2018-11-30",
            "name": "[variables('identityAppGwName')]",
            "location": "[variables('location')]"
        },
        {
            "type": "Microsoft.ManagedIdentity/userAssignedIdentities",
            "apiVersion": "2018-11-30",
            "name": "[variables('identityClusterName')]",
            "location": "[variables('location')]"
        }
    ],
    "outputs": {
        "identityAppGwID": {
            "type": "string",
            "value": "[resourceId('Microsoft.ManagedIdentity/userAssignedIdentities',variables('identityAppGwName'))]"
        },
        "appGwClientID": {
            "type": "string",
            "value": "[reference(resourceId('Microsoft.ManagedIdentity/userAssignedIdentities',variables('identityAppGwName')),'2018-11-30','Full').clientId]"
        },
        "appGwPrincipalID": {
            "type": "string",
            "value": "[reference(resourceId('Microsoft.ManagedIdentity/userAssignedIdentities',variables('identityAppGwName')),'2018-11-30','Full').principalId]"
        },
        "identityClusterID": {
            "type": "string",
            "value": "[resourceId('Microsoft.ManagedIdentity/userAssignedIdentities',variables('identityClusterName'))]"
        }
    }
}